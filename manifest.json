{
  "manifest_version": 2,

  "name": "Theme extension",
  "description": "You can setup theme for VK",
  "version": "0.0.1",

  "content_security_policy": "script-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}